{
  "name": "@luisbedoia/coolprop-wasm",
  "version": "0.0.2",
  "description": "A TypeScript wrapper for the CoolProp library using WebAssembly",
  "scripts": {
    "build": "docker-compose up --build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "coolprop",
    "typescript",
    "thermodynamics",
    "wasm"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisbedoia/coolprop-wasm.git"
  },
  "bugs": {
    "url": "https://github.com/luisbedoia/coolprop-wasm/issues"
  },
  "homepage": "https://github.com/luisbedoia/coolprop-wasm#readme",
  "type": "module",
  "main": "coolprop.cjs",
  "module": "wasm/coolprop.js",
  "types": "wasm/coolprop.d.ts",
  "exports": {
    ".": {
      "types": "./wasm/coolprop.d.ts",
      "import": "./wasm/coolprop.js",
      "require": "./coolprop.cjs",
      "default": "./wasm/coolprop.js"
    }
  },
  "files": [
    "coolprop.cjs",
    "wasm/coolprop.js",
    "wasm/coolprop.wasm",
    "wasm/coolprop.d.ts"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  }
}
